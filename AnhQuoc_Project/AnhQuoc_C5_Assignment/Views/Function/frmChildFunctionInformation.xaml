<Window x:Class="AnhQuoc_C5_Assignment.frmChildFunctionInformation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnhQuoc_C5_Assignment"
        mc:Ignorable="d"
        Title="Child Function Information" 
        Style="{StaticResource wdStyleGeneral}"
        WindowStartupLocation="{StaticResource WSL}">
    <StackPanel Width="800">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <local:ucFunctionInformation Grid.Column="0"
                                         x:Name="ucParentInformation" 
                                         Margin="0 0 5 0" />
            <local:ucFunctionInformation Grid.Column="1"
                                         x:Name="ucChildInformation" 
                                         Margin="5 0 0 0" />
        </Grid>
        <StackPanel Name="stkFooter"
                    Style="{StaticResource stkWrapButton}">
            
        </StackPanel>
    </StackPanel>
</Window>
