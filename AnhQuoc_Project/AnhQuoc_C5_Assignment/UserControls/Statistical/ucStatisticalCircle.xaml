<UserControl x:Class="AnhQuoc_C5_Assignment.UserControls.Statistical.ucStatisticalCircle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AnhQuoc_C5_Assignment.UserControls.Statistical"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Ellipse x:Name="outside"
                 StrokeThickness="1"
                 Stroke="Black"
                 Fill="Transparent"
                 Width="200"
                 Height="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Width}" />
        <Ellipse x:Name="inside"
                 StrokeThickness="1"
                 Stroke="Black"
                 Fill="Transparent"
                 Width="130"
                 Height="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Width}" />
    </Grid>
</UserControl>
