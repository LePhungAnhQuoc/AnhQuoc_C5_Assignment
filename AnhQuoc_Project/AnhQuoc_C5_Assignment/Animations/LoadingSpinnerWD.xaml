<Window x:Class="AnhQuoc_C5_Assignment.Animations.LoadingSpinnerWD"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnhQuoc_C5_Assignment.Animations" 
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        mc:Ignorable="d"
        Title="LoadingSpinnerWD"
        Style="{StaticResource wdStyleGeneral}"
        WindowStartupLocation="{StaticResource WSL}"
        WindowStyle="None"
        AllowsTransparency="True"
        Background="Transparent">
    <Grid>
        <Image Name="imgCircle" Visibility="Visible" Width="250" Height="250" gif:ImageBehavior.AnimatedSource="/Animations/LoadingSpinnerCircle2.gif" />
    </Grid>
</Window>
